<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="view.ui.fragment.TiendaFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="103dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="32dp"
        android:layout_marginRight="20dp"
        android:background="@color/color_60_principal">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/logocorporativo">
        </ImageView>
    </RelativeLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="155dp">

    </androidx.recyclerview.widget.RecyclerView>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/buttomnavegation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_60_principal"
        android:layout_marginTop="665dp"
        app:itemIconTint="@color/black"
        tools:ignore="MissingConstrains"
        app:menu="@menu/menu_navigation_bar">

    </com.google.android.material.bottomnavigation.BottomNavigationView>


</FrameLayout>